app_identifier("dev.sillykat.eve.sweet") # The iOS bundle identifier of your app
package_name("dev.sillykat.eve.sweet") # The Android bundle identifier of your app

# Set in Environment
apple_id("#{ENV["FASTLANE_APPLE_ID"]}") # Your Apple email address
itc_team_id("#{ENV["FASTLANE_ITC_TEAM_ID"]}") # App Store Connect Team ID
team_id("#{ENV["FASTLANE_TEAM_ID"]}") # Developer Portal Team ID

